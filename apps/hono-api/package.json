{
  "name": "cloudflare-workers-openapi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --port 8788",
    "dev:log": "DEBUG_HTTP_LOG=1 wrangler dev --port 8788 2>&1 | tee log.txt",
    "start": "wrangler dev --remote",
    "cf-typegen": "wrangler types",
    "db:update:local": "wrangler d1 execute tapcanvas --file=./schema.sql --local",
    "db:update:remote": "wrangler d1 execute tapcanvas --file=./schema.sql --remote"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/google": "^2.0.39",
    "@ai-sdk/openai": "^2.0.69",
    "ai": "^5.0.97",
    "chanfana": "^2.6.3",
    "hono": "^4.6.20",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "22.13.0",
    "@types/service-worker-mock": "^2.0.4",
    "wrangler": "^4.53.0"
  }
}
